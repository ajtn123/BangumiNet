<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:bn="https://github.com/ajtn123/BangumiNet"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="bn:SubjectListPagedViewModel"
             x:Class="BangumiNet.Views.SubjectListPagedView">
    <StackPanel Spacing="5">
        <bn:PageNavigatorView DataContext="{Binding PageNavigator}"/>
        <bn:SubjectListView DataContext="{Binding}"/>
        <bn:PageNavigatorView DataContext="{Binding PageNavigator}"/>
    </StackPanel>
</UserControl>
